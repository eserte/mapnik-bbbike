<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map [
<!ENTITY % entities SYSTEM "inc/entities.xml.inc">
%entities;
<!ENTITY dasharray "1,3">
<!ENTITY fzcolor "#6c0000">
]>
<Map background-color="transparent" srs="&srs900913;" minimum-version="0.7.1">
  &fontset-settings;

<Style name="unknown">
  
    <Rule>
      &maxscale_zoom10;
      &minscale_zoom12;
      <LineSymbolizer stroke="&fzcolor;" stroke-width="1"
		      stroke-dasharray="&dasharray;" />
    </Rule>
    <Rule>
      &maxscale_zoom13;
      &minscale_zoom14;
      <LineSymbolizer stroke="&fzcolor;" stroke-width="2"
		      stroke-dasharray="&dasharray;" />
    </Rule>
    <Rule>
      &maxscale_zoom15;
      &minscale_zoom16;
      <LineSymbolizer stroke="&fzcolor;" stroke-width="3"
		      stroke-dasharray="&dasharray;" />
    </Rule>
    <Rule>
      &maxscale_zoom17;
      &minscale_zoom19;
      <LineSymbolizer stroke="&fzcolor;" stroke-width="6"
		      stroke-dasharray="&dasharray;" />
      <TextSymbolizer fontset-name="book-fonts" size="9"
		      horizontal-alignment="right"
		      wrap-width="300" >[FIXME]</TextSymbolizer>
    </Rule>
    
</Style>

<!-- -->
<Layer name="unknown" status="on" srs="&osm2pgsql_projection;">
    <StyleName>unknown</StyleName>
    <Datasource>
      <Parameter name="table">
      (select way,"FIXME"
       from &prefix;_line
       where "FIXME" is not null
       order by z_order) as unknown
      </Parameter>
      &datasource-settings;
    </Datasource>
</Layer>

</Map>
